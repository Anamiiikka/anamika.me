{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Run Dev Server",
			"type": "shell",
			"command": "npm",
			"args": [
				"run",
				"dev"
			],
			"isBackground": true,
			"problemMatcher": [
				"$tsc-watch"
			],
			"group": "none"
		},
		{
			"label": "Run Dev Server (Port 3002)",
			"type": "shell",
			"command": "npm",
			"args": [
				"run",
				"dev",
				"--",
				"-p",
				"3002"
			],
			"isBackground": true,
			"problemMatcher": [
				"$tsc-watch"
			],
			"group": "none"
		}
	]
}